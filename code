   public static void SortListInteger(List<Integer> list){
    	 for (int i = 0; i < list.size() - 1; i++) {  
	            for (int j = 1; j < list.size() - i; j++) {  
	                Integer a;  
	                if ((list.get(j - 1)).compareTo(list.get(j)) > 0) { // 比较两个整数的大小  
	                    a = list.get(j - 1);  
	                    list.set((j - 1), list.get(j));  
	                    list.set(j, a);  
	                }  
	            }  
	        }  
	 }
